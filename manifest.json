{
  "manifest_version": 3,
  "name": "Video Downloader",
  "version": "1.0",
  "permissions": [
    "downloads",
    "activeTab",
    "scripting"
  ],
  "icons": {
    "16":  "icons/icon16.png",
    "32":  "icons/icon32.png",
    "48":  "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_icon": {
      "16":  "icons/icon16.png",
      "32":  "icons/icon32.png",
      "48":  "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "Download Videos"
  },
  "background": {
    "service_worker": "background.js"
  }
}